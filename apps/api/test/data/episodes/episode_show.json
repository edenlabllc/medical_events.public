{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "uuid": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$"
    },
    "period": {
      "type": "object",
      "properties": {
        "start": {
          "type": "string",
          "format": "date-time",
          "description": "C? Starting time with inclusive boundary"
        },
        "end": {
          "type": "string",
          "format": "date-time",
          "description": "C? End time with inclusive boundary, if not ongoing"
        }
      },
      "required": [
        "start"
      ],
      "additionalProperties": false
    },
    "identifier": {
      "type": "object",
      "required": [
        "identifier"
      ]
    },
    "coding": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "#/definitions/coding_item"
      }
    },
    "coding_item": {
      "type": "object",
      "properties": {
        "system": {
          "type": "string",
          "enum": [
            "eHealth/resources"
          ]
        },
        "code": {
          "type": "string",
          "description": "Symbol in syntax defined by the system"
        },
        "display": {
          "type": "string",
          "description": "Representation defined by the system"
        }
      },
      "required": [
        "code",
        "system"
      ],
      "additionalProperties": false
    },
    "codeable_concept": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "coding": {
          "$ref": "#/definitions/coding"
        },
        "text": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "required": [
        "coding"
      ],
      "additionalProperties": false
    },
    "reference": {
      "type": "object",
      "properties": {
        "identifier": {
          "type": "object",
          "properties": {
            "type": {
              "$ref": "#/definitions/codeable_concept"
            },
            "value": {
              "$ref": "#/definitions/uuid"
            }
          }
        }
      }
    },
    "diagnosis": {
      "type": "object",
      "properties": {
        "condition": {
          "$ref": "#/definitions/reference"
        },
        "role": {
          "type": "object",
          "properties": {
            "coding": {
              "type": "array",
              "minItems": 1,
              "items": {
                "type": "object",
                "properties": {
                  "system": {
                    "type": "string",
                    "enum": [
                      "eHealth/diagnosis_roles"
                    ]
                  },
                  "code": {
                    "type": "string",
                    "description": "Symbol in syntax defined by the system"
                  }
                },
                "required": [
                  "code",
                  "system"
                ],
                "additionalProperties": false
              }
            },
            "text": {
              "type": [
                "string",
                "null"
              ]
            }
          },
          "required": [
            "coding"
          ],
          "additionalProperties": false
        },
        "code": {
          "type": "object",
          "properties": {
            "coding": {
              "type": "array",
              "minItems": 1,
              "items": {
                "type": "object",
                "properties": {
                  "system": {
                    "type": "string",
                    "enum": [
                      "eHealth/ICPC2/condition_codes",
                      "eHealth/ICD10/condition_codes"
                    ]
                  },
                  "code": {
                    "type": "string",
                    "description": "Symbol in syntax defined by the system"
                  }
                },
                "required": [
                  "code",
                  "system"
                ],
                "additionalProperties": false
              }
            },
            "text": {
              "type": [
                "string",
                "null"
              ]
            }
          },
          "required": [
            "coding"
          ],
          "additionalProperties": false
        },
        "rank": {
          "type": "integer",
          "minimum": 1
        }
      },
      "additionalProperties": true
    },
    "diagnoses_history": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date"
          },
          "is_active": {
            "type": "boolean"
          },
          "evidence": {
            "$ref": "#/definitions/reference"
          },
          "diagnoses": {
            "type": "array",
            "minItems": 1,
            "items": {
              "$ref": "#/definitions/diagnosis"
            }
          }
        }
      }
    },
    "status_history": {
      "type": "array",
      "items": {
        "type": "object",
        "status": {
          "type": "string"
        },
        "status_reason": {
          "$ref": "#/definitions/codeable_concept"
        },
        "inserted_at": {
          "type": "string",
          "format": "date-time"
        },
        "inserted_by": {
          "$ref": "#/definitions/uuid"
        }
      }
    }
  },
  "type": "object",
  "properties": {
    "id": {
      "$ref": "#/definitions/uuid"
    },
    "type": {
      "type": "object",
      "properties": {
        "system": {
          "type": "string",
          "enum": [
            "eHealth/episode_types"
          ]
        },
        "code": {
          "type": "string"
        }
      },
      "required": [
        "code",
        "system"
      ],
      "additionalProperties": false
    },
    "status": {
      "type": "string",
      "enum": [
        "active"
      ],
      "description": "Dictionary: TBD"
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "period": {
      "type": "object",
      "properties": {
        "start": {
          "type": "string",
          "format": "date",
          "description": "C? Starting time with inclusive boundary"
        },
        "end": {
          "type": [
            "string",
            "null"
          ],
          "format": "date",
          "description": "C? Starting time with inclusive boundary"
        }
      },
      "required": [
        "start"
      ],
      "additionalProperties": false
    },
    "managing_organization": {
      "type": "object",
      "properties": {
        "identifier": {
          "type": "object",
          "properties": {
            "type": {
              "type": "object",
              "properties": {
                "coding": {
                  "type": "array",
                  "minItems": 1,
                  "items": {
                    "type": "object",
                    "properties": {
                      "system": {
                        "type": "string",
                        "enum": [
                          "eHealth/resources"
                        ]
                      },
                      "code": {
                        "type": "string",
                        "enum": [
                          "legal_entity"
                        ],
                        "description": "Symbol in syntax defined by the system"
                      },
                      "display": {
                        "type": "string",
                        "description": "Representation defined by the system"
                      }
                    },
                    "required": [
                      "code",
                      "system"
                    ],
                    "additionalProperties": false
                  }
                },
                "text": {
                  "type": [
                    "string",
                    "null"
                  ]
                }
              },
              "required": [
                "coding"
              ],
              "additionalProperties": false
            },
            "value": {
              "type": "string",
              "description": "The value that is unique"
            }
          },
          "required": [
            "value"
          ],
          "additionalProperties": false
        }
      }
    },
    "care_manager": {
      "type": "object",
      "properties": {
        "identifier": {
          "type": "object",
          "properties": {
            "type": {
              "type": "object",
              "properties": {
                "coding": {
                  "type": "array",
                  "minItems": 1,
                  "items": {
                    "type": "object",
                    "properties": {
                      "system": {
                        "type": "string",
                        "enum": [
                          "eHealth/resources"
                        ]
                      },
                      "code": {
                        "type": "string",
                        "enum": [
                          "employee"
                        ],
                        "description": "Symbol in syntax defined by the system"
                      },
                      "display": {
                        "type": "string",
                        "description": "Representation defined by the system"
                      }
                    },
                    "required": [
                      "code",
                      "system"
                    ],
                    "additionalProperties": false
                  }
                },
                "text": {
                  "type": [
                    "string",
                    "null"
                  ]
                }
              },
              "required": [
                "coding"
              ],
              "additionalProperties": false
            },
            "value": {
              "type": "string",
              "description": "The value that is unique"
            }
          },
          "required": [
            "value"
          ],
          "additionalProperties": false
        }
      }
    },
    "closing_summary": {
      "type": [
        "string",
        "null"
      ],
      "description": "Closing summary"
    },
    "explanatory_letter": {
      "type": [
        "string",
        "null"
      ],
      "description": "Explanatory letter"
    },
    "status_reason": {
      "$ref": "#/definitions/codeable_concept"
    },
    "diagnoses_history": {
      "$ref": "#/definitions/diagnoses_history"
    },
    "status_history": {
      "$ref": "#/definitions/status_history"
    }
  },
  "required": [
    "id",
    "type",
    "status",
    "name",
    "period",
    "managing_organization",
    "care_manager",
    "diagnoses_history",
    "inserted_at",
    "closing_summary",
    "status_reason",
    "explanatory_letter"
  ]
}
