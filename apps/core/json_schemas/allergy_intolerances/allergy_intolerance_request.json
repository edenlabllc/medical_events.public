{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "string",
      "description": "Allergy intolerance's code "
    },
    "encounter_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$"
    },
    "episode_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$"
    },
    "onset_date_time_from": {
      "type": "string",
      "format": "date",
      "description": "Selection start date"
    },
    "onset_date_time_to": {
      "type": "string",
      "format": "date",
      "description": "Selection end date"
    }
  },
  "additionalProperties": false
}
