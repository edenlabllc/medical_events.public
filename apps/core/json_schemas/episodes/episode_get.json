{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "string",
      "description": "Episode's diagnosis's code "
    },
    "period_from": {
      "type": "string",
      "format": "date",
      "description": "Start date of episode"
    },
    "period_to": {
      "type": "string",
      "format": "date",
      "description": "End date of episode"
    },
    "service_request_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$"
    },
    "status": {
      "type": "string"
    }
  },
  "additionalProperties": false
}
